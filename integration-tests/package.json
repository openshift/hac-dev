{
  "name": "@hac-dev/integration-tests",
  "version": "0.0.1",
  "description": "HAC-DEV Cypress tests",
  "private": true,
  "scripts": {
    "clean-reports": "rm -rf cypress/",
    "combine-reports": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/report.json",
    "generate-report": "marge cypress/reports/report.json -f report -o cypress/reports/mochareports",
    "posttest": "npm run combine-reports && npm run generate-report && rm -rf cypress/reports/mocha",
    "cy:run": "npm-run-all clean-reports cy:run:* posttest",
    "cy:run:hac-dev": "cypress run",
    "cy:run:hac-dev-experimental": "cypress run -e configFile=hac-dev-experimental",
    "cy:open:hac-dev": "cypress open",
    "cy:open:hac-dev-experimental": "cypress open -e configFile=hac-dev-experimental"
  },
  "devDependencies": {
    "@cypress/xpath": "^2.0.3",
    "@reportportal/agent-js-cypress": "^5.1.0",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.13",
    "axe-core": "^4.4.2",
    "cypress": "^9.6.0",
    "cypress-axe": "^0.14.0",
    "cypress-grep": "^2.14.0",
    "cypress-multi-reporters": "^1.6.0",
    "cypress-terminal-report": "^3.5.2",
    "deepmerge": "^4.2.2",
    "fs-extra": "^10.1.0",
    "mocha-junit-reporter": "^2.0.2",
    "mochawesome": "^6.2.1",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^5.1.0",
    "npm-run-all": "^4.1.5"
  }
}
