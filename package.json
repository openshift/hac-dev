{
  "name": "hac-dev",
  "version": "0.0.1",
  "private": false,
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "scripts": {
    "analyze": "NODE_ENV=production webpack --config config/prod.webpack.config.js --env analyze=true",
    "build": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "coverage": "jest --coverage --runInBand --detectOpenHandles --forceExit",
    "deploy": "npm-run-all build lint test",
    "lint": "npm-run-all lint:*",
    "lint:ts": "eslint . --ext .js,.jsx,.ts,.tsx --color",
    "lint:ts:fix": "npm run lint:ts -- --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "nightly": "npm run deploy",
    "start": "webpack serve --config config/dev.webpack.config.js",
    "start:beta": "BETA=true npm start",
    "start:beta:ci": "ENVIRONMENT=ci npm run start:beta",
    "start:prod": "REMOTE_CONFIG=ci ENVIRONMENT=prod BETA=true PROXY=true npm run start",
    "start:standalone": "STANDALONE=true npm start",
    "test": "jest",
    "verify": "npm-run-all build lint coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@patternfly/react-catalog-view-extension": "4.13.18",
    "@patternfly/react-core": "4.135.0",
    "@patternfly/react-table": "4.29.0",
    "@patternfly/react-virtualized-extension": "4.9.48",
    "@redhat-cloud-services/frontend-components": "^3.3.3",
    "@redhat-cloud-services/frontend-components-notifications": "^3.2.2",
    "@redhat-cloud-services/frontend-components-utilities": "^3.2.4",
    "classnames": "^2.3.1",
    "formik": "^2.2.9",
    "history": "4.x",
    "js-yaml": "^4.1.0",
    "lodash-es": "^4.17.21",
    "react": "17.0.2",
    "react-dnd": "^14.0.4",
    "react-dnd-html5-backend": "^14.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.12.0",
    "react-redux": "7.2.4",
    "react-router-dom": "5.2.0",
    "react-virtualized": "^9.22.3",
    "redux": "4.1.0",
    "redux-logger": "3.0.6",
    "redux-promise-middleware": "6.1.2",
    "showdown": "^1.9.0"
  },
  "devDependencies": {
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "^1.2.1",
    "@redhat-cloud-services/frontend-components-config": "^4.3.5",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/enzyme": "^3.10.10",
    "@types/history": "^4.7.9",
    "@types/jest": "^27.0.2",
    "@types/lodash-es": "4.17.x",
    "@types/react-helmet": "5.x",
    "@types/react-router-dom": "^5.3.2",
    "@types/react-virtualized": "9.x",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.2",
    "enzyme": "3.11.0",
    "enzyme-to-json": "3.6.2",
    "eslint": "7.29.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.0.5",
    "npm-run-all": "4.1.5",
    "prop-types": "15.7.2",
    "stylelint": "13.13.1",
    "stylelint-config-recommended-scss": "4.2.0",
    "stylelint-scss": "3.19.0",
    "ts-jest": "^27.0.7",
    "typescript": "4.4.4",
    "webpack-bundle-analyzer": "4.4.2"
  },
  "insights": {
    "appname": "hac-dev"
  }
}
